<%= form_for :import, :url=>{:controller=>"admin/techniques", :action=>"do_csv_import"}, :html => { :multipart => true } do |f| %>
 <table>
   <tr>
     <td>
      <label for="Technique Type">
        Technique Type:
      </label>
     </td>
     <td >
       <%= f.collection_select :technique_type_id, TechniqueType.all, :id, :name -%>
     </td>
   </tr>
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end %>