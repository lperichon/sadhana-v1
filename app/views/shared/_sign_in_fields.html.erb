<div class='field'>
  <%= f.label :email %><br/>
  <%= f.text_field :email %>
</div>
<div class='field'>
<%= f.label :password %><br/>
<%= f.password_field :password %>
</div>
<% if devise_mapping && devise_mapping.rememberable? %>
<div class='field'>
  <%= f.check_box :remember_me %> <%= f.label :remember_me %>
</div>
<% end %>